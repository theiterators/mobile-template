{
  "name": "placeholdername",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "react-native start",
    "ios:staging": "react-native run-ios --scheme 'PlaceholderName staging'",
    "ios:production": "react-native run-ios --scheme 'PlaceholderName'",
    "android:staging:debug": "react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "android:staging:release": "react-native run-android --mode=stagingRelease --appIdSuffix=staging --active-arch-only",
    "android:production:debug": "react-native run-android --mode=productionDebug --appIdSuffix=production",
    "android:production:release": "react-native run-android --mode=productionRelease --appIdSuffix=production --active-arch-only",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "prettier --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint index.js App.js app test --fix --ext .js,.ts,.tsx && npm run format",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx,json}\"",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:stage:android:maestro": "maestro test .maestro/login.android.yaml",
    "test:stage:ios:maestro": "maestro test .maestro/login.ios.yaml",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "deploy:stage:android": "fastlane local_deploy buildType:staging platform:android",
    "deploy:stage:ios": "fastlane local_deploy buildType:staging platform:ios",
    "deploy:production:android": "fastlane local_deploy buildType:production platform:android",
    "deploy:production:ios": "fastlane local_deploy buildType:production platform:ios",
    "clean": "npx react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "postversion": "react-native-version --legacy --never-amend",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "prepare": "husky"
  },
  "release-it": {
    "git": {
      "requireUpstream": false,
      "requireCleanWorkingDir": false,
      "commitMessage": "chore: release v${version}",
      "changelog": "git log $(git describe --tags --abbrev=0 @^)..@ --merges --pretty=format:\"* %s\" | sed -n 's/Merge branch '\\''\\(.*\\)'\\'' into.*/- \\1/p'"
    },
    "npm": {
      "ignoreVersion": false,
      "publish": false
    },
    "gitlab": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
