{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "local",
    "requireCommit": true,
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "base": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleStagingRelease",
        "image": "latest"
      },
      "ios": {
        "image": "latest",
        "scheme": "placeholdername staging"
      }
    },
    "previewStaging": {
      "extends": "base"
    },
    "previewProduction": {
      "extends": "base",
      "ios": {
        "scheme": "placeholdername"
      },
      "android": {
        "gradleCommand": ":app:assembleProductionRelease",
        "image": "latest"
      }
    },
    "production": {
      "extends": "previewProduction",
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "bundleIdentifier": "com.placeholdername",
        "ascApiKeyId": "APP_ID_CODE",
        "appleTeamId": "E2KG6LTEW8"
      },
      "android": {
        "applicationId": "com.placeholdername",
        "serviceAccountKeyPath": "google.json/path",
        "track": "internal"
      }
    }
  }
}
